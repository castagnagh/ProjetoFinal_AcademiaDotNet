@model ProjetoFinal.Models.Computador

@{
    ViewData["Title"] = "Detalhes Computador";
}

<p class="display-6 mt-3">Detalhes</p>


<div>
    <h4>Computador</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Nr Patrimônio
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.NumPatrimonio)
        </dd>
        <dt class="col-sm-2">
            Descrição
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Descricao)
        </dd>
        <dt class="col-sm-2">
            Seção
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Secao.Nome)
        </dd>
        <dt class="col-sm-2">
            Marca
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Marca.Nome)
        </dd>
        <dt class="col-sm-2">
            Tipo
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.TipoComputador.Nome)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Index" class="btn btn-outline-secondary btn-sm">Voltar</a>
</div>
